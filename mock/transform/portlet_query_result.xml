                <nsql dbId="Niku" dbVendor="all">SELECT  @SELECT:DIM:USER_DEF:IMPLIED:APPLICATION:b.sort:sort@,
                    @SELECT:DIM_PROP:USER_DEF:IMPLIED:APPLICATION:b.label:label@,
                    @SELECT:DIM_PROP:USER_DEF:IMPLIED:APPLICATION:b.app_count:app_count@,
                    @SELECT:DIM_PROP:USER_DEF:IMPLIED:APPLICATION:b.code:code@
                    FROM (
                    SELECT count(DISTINCT inv.id) app_count, &apos;Good Fit&apos; label, 1 sort, &apos;Q1&apos; code
                    FROM INV_INVESTMENTS inv
                    INNER JOIN ODF_APPLICATION_V2 a on inv.id = a.odf_pk
                    WHERE a.apm_business_score &gt;= 50 AND a.apm_technology_score &gt;= 50
                    AND (@WHERE:PARAM:USER_DEF:STRING:install_type@ IS NULL OR @WHERE:PARAM:USER_DEF:STRING:install_type@ = a.apm_install_type)
                    AND (@WHERE:PARAM:USER_DEF:INTEGER:vendor@ IS NULL OR @WHERE:PARAM:USER_DEF:INTEGER:vendor@ = a.apm_vendor)
                    AND (@WHERE:PARAM:USER_DEF:STRING:category_filter@ IS NULL OR @WHERE:PARAM:USER_DEF:STRING:category_filter@ = a.category_code)
                    UNION
                    SELECT count(DISTINCT inv.id), &apos;Poor Technology Score&apos; label, 2 sort, &apos;Q4&apos; code
                    FROM INV_INVESTMENTS inv
                    INNER JOIN ODF_APPLICATION_V2 a on inv.id = a.odf_pk
                    WHERE a.apm_business_score &gt;= 50 AND a.apm_technology_score &lt; 50
                    AND (@WHERE:PARAM:USER_DEF:STRING:install_type@ IS NULL OR @WHERE:PARAM:USER_DEF:STRING:install_type@ = a.apm_install_type)
                    AND (@WHERE:PARAM:USER_DEF:INTEGER:vendor@ IS NULL OR @WHERE:PARAM:USER_DEF:INTEGER:vendor@ = a.apm_vendor)
                    AND (@WHERE:PARAM:USER_DEF:STRING:category_filter@ IS NULL OR @WHERE:PARAM:USER_DEF:STRING:category_filter@ = a.category_code)
                    UNION
                    SELECT count(DISTINCT inv.id), &apos;Poor Business Value&apos; label, 3 sort, &apos;Q2&apos; code
                    FROM INV_INVESTMENTS inv
                    INNER JOIN ODF_APPLICATION_V2 a on inv.id = a.odf_pk
                    WHERE a.apm_business_score &lt; 50 AND a.apm_technology_score &gt;= 50
                    AND (@WHERE:PARAM:USER_DEF:STRING:install_type@ IS NULL OR @WHERE:PARAM:USER_DEF:STRING:install_type@ = a.apm_install_type)
                    AND (@WHERE:PARAM:USER_DEF:INTEGER:vendor@ IS NULL OR @WHERE:PARAM:USER_DEF:INTEGER:vendor@ = a.apm_vendor)
                    AND (@WHERE:PARAM:USER_DEF:STRING:category_filter@ IS NULL OR @WHERE:PARAM:USER_DEF:STRING:category_filter@ = a.category_code)
                    UNION
                    SELECT count(DISTINCT inv.id), &apos;Poor Fit&apos; label, 4 sort, &apos;Q3&apos; code
                    FROM INV_INVESTMENTS inv
                    INNER JOIN ODF_APPLICATION_V2 a on inv.id = a.odf_pk
                    WHERE a.apm_business_score &lt; 50 AND a.apm_technology_score &lt; 50
                    AND (@WHERE:PARAM:USER_DEF:STRING:install_type@ IS NULL OR @WHERE:PARAM:USER_DEF:STRING:install_type@ = a.apm_install_type)
                    AND (@WHERE:PARAM:USER_DEF:INTEGER:vendor@ IS NULL OR @WHERE:PARAM:USER_DEF:INTEGER:vendor@ = a.apm_vendor)
                    AND (@WHERE:PARAM:USER_DEF:STRING:category_filter@ IS NULL OR @WHERE:PARAM:USER_DEF:STRING:category_filter@ = a.category_code)
                    ) b
