    <xogtype type="login">
        <soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:obj="http://www.niku.com/xog/Object">
            <soapenv:Header/>
            <soapenv:Body>
                <obj:Login>
                    <obj:Username />
                    <obj:Password />
                </obj:Login>
            </soapenv:Body>
        </soapenv:Envelope>
    </xogtype>
