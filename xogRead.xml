<?xml version="1.0" encoding="utf-8"?>
<xogread version="1.0">
    <xogtype type="requestLogin">
        <username />
        <password />
    </xogtype>
    <xogtype type="login">
        <soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:obj="http://www.niku.com/xog/Object">
            <soapenv:Header/>
            <soapenv:Body>
                <obj:Login>
                    <obj:Username />
                    <obj:Password />
                </obj:Login>
            </soapenv:Body>
        </soapenv:Envelope>
    </xogtype>
    <xogtype type="logout">
        <soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:obj="http://www.niku.com/xog/Object">
            <soapenv:Header/>
            <soapenv:Body>
                <obj:Logout>
                    <obj:SessionID />
                </obj:Logout>
            </soapenv:Body>
        </soapenv:Envelope>
    </xogtype>
    <xogtype type="envelope">
        <soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:xog="http://www.niku.com/xog">
            <soapenv:Header>
                <xog:Auth>
                    <xog:SessionID />
                </xog:Auth>
            </soapenv:Header>
            <soapenv:Body />
        </soapenv:Envelope>
    </xogtype>
    <xogtype type="Lookups">
        <NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_read.xsd">
            <Header action="read" externalSource="NIKU" objectType="contentPack" version="8.0">
                <args name="no_dependencies" value="true"/>
            </Header>
            <LookupQuery>
                <Filter criteria="EQUALS" name="code"/>
            </LookupQuery>
        </NikuDataBus>
    </xogtype>
    <xogtype type="Objects">
        <NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_read.xsd">
            <Header action="read" externalSource="NIKU" objectType="contentPack" version="8.0">
                <args name="no_dependencies" value="true"/>
                <args name="exclude_parent" value="true"/>
                <args name="noparents" value="true"/>
            </Header>
            <ObjectQuery>
                <Filter criteria="EQUALS" name="object_code"/>
            </ObjectQuery>
        </NikuDataBus>
    </xogtype>
    <xogtype type="Views">
        <NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_read.xsd">
            <Header action="read" externalSource="NIKU" objectType="contentPack" version="8.0">
                <args name="no_dependencies" value="true"/>
            </Header>
            <ViewQuery>
                <Filter criteria="EQUALS" name="code"/>
                <Filter criteria="EQUALS" name="object_code"/>
                <Filter criteria="EQUALS" name="partition_code"/>
            </ViewQuery>
        </NikuDataBus>
    </xogtype>
    <xogtype type="Portlets">
        <NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_read.xsd">
